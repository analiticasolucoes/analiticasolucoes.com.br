{
    "name": "analitica-solucoes/mvc-system",
    "description": "Sistema MVC personalizado para Analítica Soluções",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Analítica Soluções",
            "email": "contato@analiticasolucoes.com.br"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "config/app.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php -r \"echo 'Autoload atualizado com sucesso!\\n';\""
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}